        fjs.parentNode.insertBefore(js, fjs);
      }(document, 'script', 'facebook-jssdk'));</script>
      
    <div class="wrapper">
      <%= render 'layouts/header' %>
      <%= yield %>
      <div class="push"></div>
    </div>

  <div class="footer">
    <%= render 'layouts/footer' %>
  </div>
  </body>

</html>
